	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"baseUrl": "./",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"isolatedModules": true,
		"module": "esnext",
		"moduleResolution": "bundler",
		"outDir": "Target",
		"rootDir": "Source",
		"skipLibCheck": true,
		"strict": true,
		"target": "esnext"
	},
	"extends": "@playform/build/tsconfig",
	"include": [
		"Source",
		"Source",
		"Source",
		"Source",
		"Source",
		"Source"
	]
}
